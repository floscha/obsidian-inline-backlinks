{
    "name": "obsidian-inline-backlinks",
    "version": "1.0.0",
    "description": "An Obsidian plugin that displays backlinks inline above the note content",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "backlinks"
    ],
    "author": "Florian Schaefer",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "builtin-modules": "^3.3.0",
        "esbuild": "0.17.3",
        "obsidian": "latest",
        "tslib": "^2.8.1",
        "typescript": "4.7.4"
    }
}